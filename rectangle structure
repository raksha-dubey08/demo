#include <stdio.h>

// Define the Rectangle structure
struct Rectangle {
    int length;
    int breadth;
};

// Function to take input and return a Rectangle
struct Rectangle inputRectangle() {
    struct Rectangle r;
    printf("Enter length: ");
    scanf("%d", &r.length);
    printf("Enter breadth: ");
    scanf("%d", &r.breadth);
    return r;
}

// Function to calculate area of a Rectangle
int area(struct Rectangle r) {
    return r.length * r.breadth;
}

int main() {
    struct Rectangle rect;

    // Input rectangle
    rect = inputRectangle();

    // Calculate and display area
    int rectArea = area(rect);
    printf("Area of the rectangle: %d\n", rectArea);

    return 0;
}
